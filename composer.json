    "keywords": [
        "psr-7",
        "request",
        "response",
        "response-factory",
        "content-negotiation"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/negotiation",
    "support": {
        "issues": "https://github.com/ellipsephp/negotiation/issues",
        "source": "https://github.com/ellipsephp/negotiation"
    },
